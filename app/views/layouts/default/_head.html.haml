%title
  = current_setup.title
- if current_setup.favicon.try(:image).nil?
  = favicon_link_tag 'favicon.png', type: 'image/png'
- else
  = favicon_link_tag current_setup.favicon.image.encode(:png).thumb('32x32').url, type: 'image/png'
%meta{ :name => 'description', :content => current_setup.description }
%meta{ :name => 'keywords', :content => current_setup.keywords }
%meta{ :name => 'author', :content => 'GINA' }

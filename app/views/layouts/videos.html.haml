!!! 5
%html
%head
  %title North Slope Science Catalog
  %link{ :href => asset_path('favicon3.ico'), :rel => "SHORTCUT ICON"}
  %meta{ :name => 'description', :content => 'North Slope Science Initiative Catalog' }
  %meta{ :name => 'keywords', :content => 'North Slope Science Initiative Catalog data projects tracking geographic information network alaska' }
  %meta{ :name => 'author', :content => 'GINA' }

  = stylesheet_link_tag 'application', :media => 'screen, projection'
  = stylesheet_link_tag 'print', :media => 'print'
  /[if IE]
    = stylesheet_link_tag 'ie', :media => 'screen, projection'

  = javascript_include_tag "prototype", "application"

%body
  #header
    = render 'layouts/header'
  #content.x-hidden
    = yield
  #footer.x-hidden
    = render 'layouts/footer'
  %script
    -#= render 'layouts/regions.js'
  = render 'layouts/analytics'
- theme = 'settings'
!!! 5
%html
%head
  = render "layouts/#{theme}/head"  
  = javascript_include_tag "application", data: {'turbolinks-track' => ''}
  = stylesheet_link_tag 'application', data: {'turbolinks-track' => ''}
%body
  .container
    #header= render "layouts/#{theme}/header"
    #content
      = show_flash_messages
      = yield
    #footer= render "layouts/#{theme}/footer"
  = render "layouts/#{theme}/analytics" unless Rails.env == 'development'

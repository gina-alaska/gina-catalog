.row-fluid
  .span12
    %h4.pull-left
      - if controller.class.const_defined?(:PAGETITLE)
        .brand= "Portal Settings: #{controller.class::PAGETITLE}"
      - else
        .brand= "Portal Settings"
    .navbar.navbar-static-top
      .navbar-inner
        - if @edit_button == true
          = link_to edit_manager_setup_path, class: 'btn pull-right', title: "Edit Settings" do
            %i.icon-edit

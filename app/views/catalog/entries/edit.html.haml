.container
  .panel.panel-default
    .panel-heading{ style: 'position: relative' }
      %ol.breadcrumb
        %li
          = link_to [:catalog, :entries] do
            %i.fa.fa-cubes
            Catalog Records
        %li.active
          Edit #{@entry.title}

    .panel-body.entry-form-container
      = render 'form', collection: @entry 

%tr
  %td
    = agency.name
  %td
    = agency.acronym
  %td
    = agency.description
  %td
    = link_to edit_manager_agency_path(agency), class: 'btn btn-sm btn-primary', title: 'Edit' do
      %i.fa.fa-edit
    = link_to manager_agency_path(agency), data: { method: :delete, confirm: "Are you sure?" }, class: 'btn btn-sm btn-danger', title: 'Delete' do
      %i.fa.fa-trash-o

- entry_orgs.each do |entry_org|
  - org = entry_org.organization
  .org-thumbnails{ title: org.name }
    .logo
      - if org.logo.nil?
        = image_tag image_path('blank_image.png'), style: 'width: 50px; height: 50px'
      - else
        = image_tag org.logo.thumb('50x50').url
    %h5.title
      = org.acronym
      - if entry_org.primary?
        %br/
        .label.label-primary Primary
      - if entry_org.funding?
        %br/
        .label.label-info Funding
